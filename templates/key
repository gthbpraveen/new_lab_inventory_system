from app import app, db
from models import User

with app.app_context():
    # Create a new test user
    test_user = User(
        email="cs26btech11011@iith.ac.in",
        password="12345678",  # You can use a hash in real use
        role="student",
        is_approved=True
    )
    db.session.add(test_user)
    db.session.commit()

    print(f"New user created with ID: {test_user.id}")
